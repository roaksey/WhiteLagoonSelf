@model IEnumerable<WhiteLagoon.Domain.Entities.Villa>
<h1>Villa List</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>Price </th>
            <th>Sqft </th>
            <th>Occupancy</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Price</td>
                <td>@item.Sqft</td>
                <td>@item.Occupancy</td>
            </tr>
        }
    </tbody>
</table>